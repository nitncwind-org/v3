{
  "name": "nitncwind",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --mode development",
    "prod": "vue-cli-service build",
    "pre": "vue-cli-service build --mode prerelease",
    "lint": "vue-cli-service lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.vue": [
      "vue-cli-service lint"
    ]
  },
  "dependencies": {
    "axios": "^0.21.2",
    "core-js": "^3.6.4",
    "hooper": "^0.3.4",
    "node-sass": "^8.0.0",
    "vue": "^2.6.11",
    "vue-axios": "^2.1.5",
    "vue-gtag": "^1.10.0",
    "vue-router": "^3.1.6",
    "vue-tweet-embed": "^2.4.0",
    "vuetify": "^2.2.11"
  },
  "devDependencies": {
    "@mdi/font": "^5.1.45",
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "pre-commit": "^1.2.2",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "~2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/max-attributes-per-line": [
        "off"
      ],
      "vue/no-v-html": [
        "off"
      ],
      "no-trailing-spaces": "warn"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
